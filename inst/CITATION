
# CITATION file

citHeader("To cite package 'photoSpec' in publications use:")

desc <- packageDescription("photoSpec")
year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", desc$Date)
vers <- paste("R package version", desc$Version)

citEntry(entry = "Manual",
	title = "photoSpec: A photographic spectrophotometer",
	author = personList(as.person("Bryan A. Hanson")),
	year = year,
	note = vers,
	textVersion = paste("Bryan A. Hanson (", year, ") photoSpec: A photographic spectrophotomer. ",
		vers, sep = ""))
